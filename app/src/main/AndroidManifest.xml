<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yng.appmascotas">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".SelCuentaInstagram"
            android:parentActivityName=".MainActivity">

            <!-- Soporte para versiones 4.0 om menores -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".MyBioActivity"
            android:parentActivityName=".MainActivity">

            <!-- Soporte para versiones 4.0 om menores -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ContactoActivity"
            android:parentActivityName=".MainActivity">

            <!-- Soporte para versiones 4.0 om menores -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".RaitingMascotas"
            android:parentActivityName=".MainActivity">

            <!-- Soporte para versiones 4.0 om menores -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <receiver android:name=".actions.VerFotosCuenta">
            <intent-filter>
                <action android:name="VER_FOTOS_CUENTA"/>
            </intent-filter>
        </receiver>

        <receiver android:name=".actions.CambiarUsuario">
            <intent-filter>
                <action android:name="CAMBIAR_USUARIO"/>
            </intent-filter>
        </receiver>

        <receiver android:name=".actions.DarLikeOtra">
            <intent-filter>
                <action android:name="DAR_LIKE_OTRA"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>